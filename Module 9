7.


#include<stdio.h>
int main()
{
    int n,np=0,marks;
    scanf("%d",&n);
    while(n>0){
        scanf("%d",&marks);
        if (marks>=50){
            np=np+1;
        }
        else{
            np=np+0;
        }
        n--;
    }
    printf("%d",np);
    return 0;
}



8.


#include<stdio.h>
int main()
{
    int i,j,row;
    scanf("%d",&row);
    for (i=1;i<=row;i++){
        for (j = 1; j <= i;j++)
        {
            printf("%d",j);
        }
        printf("\n");
    }
    return 0;
}



9.


#include<stdio.h>
int main()
{
    int i,j,row,num=1;
    scanf("%d",&row);
    for (i=1;i<=row;i++){
        for (j = 1; j <= i;j++)
        {   
            printf("%d",num );
            num++;
             if(i!=j){
                 printf(" ");
             }
        }
        printf("\n");
    }
    return 0;
}
