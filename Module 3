int main()
{   float P,T,R,total;
    scanf("%f %f %f",&P,&T,&R);
    total=P*T*R/100;
    printf("%.2f", total);
    /* code */
    return 0;
}


40.

#include<stdio.h>
int main()
{   int a,total=0,b;
    scanf("%d",&a);
    b=a%10;
    total=total+b;
    a=a/10;
    b=a%10;
    total=total+b;
    a=a/10;
    total=total+a;
    printf("%d",total);
    return 0;
}





