#include <stdio.h>
/* Include other headers as needed */
int main(){   
    int a,b,c=0;
    scanf("%d",&a);
    scanf("%d",&b);
    c=a;
    a=b;
    b=c;
    printf("%d\n", a);
    printf("%d", b);
    return 0;
}


