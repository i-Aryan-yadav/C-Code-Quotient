#include <stdio.h>

int main()
{
	  int l,b,perimeter,area;
	  scanf("%d %d" , &l, &b);
	  perimeter=2*(l+b);
	  area=(l*b);
	  printf("%d %d",area,perimeter);
    
    return 0;
}


19. 


#include <stdio.h>
int main()
{
	  float r,dia,area,pie;
	  scanf("%f" , &r);
	  dia=2*r;
	  pie=3.14;
	  area = pie*r*r;
	  printf("%.2f %.2f",dia,area);
    return 0;
}


