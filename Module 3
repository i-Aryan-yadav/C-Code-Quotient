38.

#include <stdio.h>
#include<math.h>
int main()
{   int side;
    float area,constant;
    scanf("%d",&side);
 	  constant= pow(3,0.5)/4;
    area=pow(side,2);
    area=area*constant;
    printf("%.2f",area);
    return 0;
}


