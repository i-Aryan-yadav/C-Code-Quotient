34. 


#include <stdio.h>
/* Include other headers as needed */
int main(){   
    int s1,s2,s3,s4,s5,sum;
    scanf("%d %d %d %d %d",&s1,&s2,&s3,&s4,&s5);
    sum=s1+s2+s3+s4+s5;
    printf("%d ",sum);
    printf("%d ",sum/5);
    return 0;
}


35.


#include <stdio.h>
/* Include other headers as needed */
int main(){   
    int a,b;
    scanf("%d\n",&a);
    scanf("%d",&b);
    a=a+b;
    b=a-b;
    a=a-b;
    printf("%d\n", a);
    printf("%d", b);
    return 0;
}


36.


#include<stdio.h>
int main()
{
    int basic_salary,gross_salary,da,hra;
    scanf("%d",&basic_salary);
    da=basic_salary*20/100;
    hra=basic_salary*10/100;
    gross_salary= basic_salary+hra+da;
    printf("%d",gross_salary);
    return 0;
}


